generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:1234@localhost:5432/MallCatalog?schema=public"
}

model account {
  account_id String  @id(map: "account_pk") @db.VarChar(40)
  slug       String  @db.VarChar(100)
  name       String  @db.VarChar(100)
  plan_id    Int
  domain     String  @db.VarChar(100)
  status     Int
  capture_id Int
  email      String  @db.VarChar(100)
  logo       String? @db.VarChar(500)
 
}


model user {
  id                String   @id @unique @default(uuid()) @db.VarChar(255)
  account_id        String    @db.VarChar(40)
  perfil_id         Int
  usuario           String    @db.VarChar(50)
  senha             String    @db.VarChar(200)
  email             String    @db.VarChar(100)
  data_habilitao    DateTime  @default(now())
  data_desabilitado DateTime? @db.Date
 
}
